@model IEnumerable<AdminPanelCore.ENTITIES.Concrete.Category>
<div class="d-flex flex-column flex-md-row align-items-center p-3 px-md-4 mb-3 bg-white border-bottom shadow-sm">
    <h5 class="my-0 mr-md-auto font-weight-normal "><b>FİRMA ADI</b></h5>
    <nav class="my-2 my-md-0 mr-md-3">
        @foreach (var item in Model)
        {
        <a class="p-2 text-dark" href="/@item.SeoLink">@item.CategoryName</a>
        }
    </nav>
    <a class="btn btn-outline-primary" href="/Login">Giriş</a>
</div>